{
  "applications": {
    "gecko": {
      "strict_min_version": "58.0a1"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "BulkTabsExtension",
    "default_popup": "tabs.html"
  },
  "description": "",
  "homepage_url": "https://github.com/Slad3",
  "manifest_version": 2,
  "name": "Bulk Tabs",
  "icons": {
	"48": "icon.png",
	"96": "icon@2x.png"
  },
  "permissions": ["tabs", "*://developer.mozilla.org/*", "webRequest"],
  "version": "1.0"
}
